{
    "name": "arrounded/:package_name",
    "description": "The description of the module",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Madewithlove",
            "email": "heroes@madewithlove.be"
        }
    ],
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "2.0.*@dev",
        "phpunit/phpunit": "^4.7",
        "mockery/mockery": "^0.9.4",
        "phpspec/phpspec": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Arrounded\\:package_name\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arrounded\\:package_name\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "lint": "php-cs-fixer fix"
    }
}
